{
  "name": "wy-helper",
  "author": "wy2010344",
  "version": "0.1.2",
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tokenParser": {
      "types": "./dist/tokenParser/index.d.ts",
      "import": "./dist/tokenParser.mjs",
      "require": "./dist/tokenParser.js"
    },
    "./infixLang": {
      "types": "./dist/infixLang/index.d.ts",
      "import": "./dist/infixLang.mjs",
      "require": "./dist/infixLang.js"
    },
    "./kanren": {
      "types": "./dist/kanren.d.ts",
      "import": "./dist/kanren.mjs",
      "require": "./dist/kanren.js"
    },
    "./Vue": {
      "types": "./dist/Vue.d.ts",
      "import": "./dist/Vue.mjs",
      "require": "./dist/Vue.js"
    }
  },
  "files": [
    "dist",
    "src",
    "tokenParser",
    "kanren",
    "Vue"
  ],
  "scripts": {
    "build": "tsc && vite build && sh './scripts/postbuild.sh'",
    "prepare": "yarn build"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.1"
  }
}